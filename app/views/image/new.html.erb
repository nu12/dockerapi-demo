<h1>Pull Image</h1>
<%= form_with(url: "/image", method: "post", local: true) do |form| %>
  
  <div class="field">
    <%= form.label :repo %>
    <%= form.text_field :repo %>
  </div>

  <div class="field">
    <%= form.label :tag %>
    <%= form.text_field :tag %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
